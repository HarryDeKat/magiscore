<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.sj3rd.magiscore" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="LaunchMyApp">
        <param name="android-package" value="nl.xservices.plugins.LaunchMyApp" />
    </feature>
    <name short="Magiscore">Magiscore - Magister statistiek</name>
    <description>
        Magiscore very nice 10/10.
    </description>
    <author email="sjoerdabolten@gmail.com" href="https://netlob.app">
        Sj3rd
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="m6loapp:*" />
    <icon density="hdpi" src="icons/res/mipmap-hdpi/ic_launcher.png" />
    <icon density="mdpi" src="icons/res/mipmap-mdpi/ic_launcher.png" />
    <icon density="xhdpi" src="icons/res/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="icons/res/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="icons/res/mipmap-xxxhdpi/ic_launcher.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="BackgroundColor" value="0xff0096db" />
    <preference name="LoadingDialog" value="Oeps,Er ging iets fout..." />
</widget>
